{
  "kind": "collectionType",
  "collectionName": "stories",
  "info": {
    "singularName": "story",
    "pluralName": "stories",
    "displayName": "Story"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "cover": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "image": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "story_view": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::story-view.story-view",
      "mappedBy": "story"
    }
  }
}
