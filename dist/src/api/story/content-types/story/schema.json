{
    "kind": "collectionType",
    "collectionName": "stories",
    "info": {
        "singularName": "story",
        "pluralName": "stories",
        "displayName": "Story"
    },
    "options": {
        "draftAndPublish": true
    },
    "pluginOptions": {},
    "attributes": {
        "title": {
            "type": "string"
        },
        "content": {
            "type": "text"
        },
        "image": {
            "type": "media",
            "multiple": true,
            "allowedTypes": [
                "images",
                "files"
            ]
        },
        "order": {
            "type": "integer"
        },
        "isActive": {
            "type": "boolean",
            "default": true
        },
        "cover": {
            "type": "media",
            "multiple": false,
            "allowedTypes": [
                "images",
                "files"
            ]
        }
    }
}
